package Thread_;

public class Test07 {
	public static void main(String[] args) {
		Phone2 call=new Phone2();
		call.start();		
		try {
			call.join();
		} catch (InterruptedException e) {
			System.out.println("?");}
		for(int i=1;i<100;i++) System.out.println("À½½Ä¸Ô±â : "+i);
	}
}
class Phone2 extends Thread{
	@Override
	public void run() {
		
		for(int i=100;i>0;i--)  System.out.println("Ä«Åå ÇÏ±â: "+i);
	}
}
